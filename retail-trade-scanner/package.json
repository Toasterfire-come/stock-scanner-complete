{
  "name": "retail-trade-scanner-theme",
  "version": "1.0.0",
  "description": "A premium stock scanning, screening, and dashboarding platform theme",
  "main": "assets/js/main.js",
  "scripts": {
    "dev": "npm run watch",
    "build": "npm run build:css && npm run build:js",
    "build:css": "sass assets/scss/main.scss assets/css/main.css --style compressed --source-map",
    "build:js": "webpack --mode production",
    "watch": "concurrently \"npm run watch:css\" \"npm run watch:js\"",
    "watch:css": "sass assets/scss/main.scss assets/css/main.css --watch --source-map",
    "watch:js": "webpack --mode development --watch",
    "lint:css": "stylelint assets/scss/**/*.scss --fix",
    "lint:js": "eslint assets/js/**/*.js --fix",
    "optimize:images": "imagemin assets/images/**/* --out-dir=assets/images/optimized",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "keywords": [
    "wordpress",
    "theme",
    "stock-trading",
    "dashboard",
    "glassmorphism",
    "dark-mode"
  ],
  "author": "Retail Trade Scanner Team",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.52.0",
    "eslint-config-standard": "^17.1.0",
    "imagemin": "^8.0.1",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^9.0.2",
    "imagemin-svgo": "^10.0.1",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss": "^8.4.31",
    "postcss-loader": "^7.3.3",
    "postcss-preset-env": "^9.3.0",
    "sass": "^1.69.5",
    "sass-loader": "^13.3.2",
    "stylelint": "^15.11.0",
    "stylelint-config-standard-scss": "^11.1.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "chart.js": "^4.4.0",
    "gsap": "^3.12.2"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}